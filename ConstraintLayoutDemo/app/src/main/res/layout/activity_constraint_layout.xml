<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraint_layout_parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:layout_margin="5dp">

    <TextView
        android:text="Call forward"
        android:layout_width="90dp"
        android:layout_height="37dp"
        android:id="@+id/textView8"
        app:layout_constraintRight_toRightOf="@+id/imageView8"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView6" />

    <TextView
        android:text="Call Locked"
        android:layout_width="90dp"
        android:layout_height="37dp"
        android:id="@+id/textView7"
        app:layout_constraintLeft_toLeftOf="@+id/imageView9"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <ImageView
        android:layout_width="93dp"
        android:layout_height="67dp"
        app:srcCompat="@android:drawable/stat_sys_phone_call_on_hold"
        android:id="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        android:layout_marginLeft="8dp"
        app:layout_constraintRight_toLeftOf="@+id/imageView8"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:layout_width="92dp"
        android:layout_height="67dp"
        app:srcCompat="@android:drawable/stat_sys_vp_phone_call"
        android:id="@+id/imageView9"
        app:layout_constraintLeft_toLeftOf="@+id/imageView2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="216dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/imageView2"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="25dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="25dp"
        android:layout_marginStart="25dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="25dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <ImageView
        android:layout_width="94dp"
        android:layout_height="67dp"
        app:srcCompat="@android:drawable/stat_sys_phone_call_forward"
        android:id="@+id/imageView8"
        app:layout_constraintRight_toRightOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView10"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:text="Call paused"
        android:layout_width="90dp"
        android:layout_height="37dp"
        android:id="@+id/textView6"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView7"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        android:layout_marginLeft="8dp"
        app:layout_constraintRight_toLeftOf="@+id/textView8"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp" />

</android.support.constraint.ConstraintLayout>

